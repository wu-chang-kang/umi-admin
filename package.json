{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.0.11",
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.9.2",
    "@types/nprogress": "^0.2.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.1",
    "ahooks": "^2.9.2",
    "antd": "^4.9.2",
    "antd-img-crop": "^3.13.2",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "lint-staged": "^10.0.7",
    "nprogress": "^0.2.0",
    "particles-bg": "^2.5.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scrollbars-custom": "^4.0.25",
    "umi": "^3.3.1",
    "umi-plugin-keep-alive": "^0.0.1-beta.18",
    "use-immer": "^0.4.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "less-vars-to-js": "^1.3.0"
  }
}
